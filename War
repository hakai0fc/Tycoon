local FuncoesEspeciaisTab = Window:MakeTab({
    Name = "FUNÃ‡Ã•ES OP",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
FuncoesEspeciaisTab:AddSection({ Name = "PODERES" })
local function safeRestoreHumanoid(humanoid)
    if humanoid and humanoid.Parent then
        pcall(function()
            humanoid.PlatformStand = false
            humanoid.AutoRotate = true
        end)
    end
end
local function safeStopFly()
    if flyConnection then flyConnection:Disconnect() flyConnection=nil end
    if BG then BG:Destroy() BG=nil end
    if BV then BV:Destroy() BV=nil end
    local char = LP.Character
    if char then
        safeRestoreHumanoid(char:FindFirstChildOfClass("Humanoid"))
    end
end
local function startFly(char)
    safeStopFly()
    local humanoid = char:FindFirstChildOfClass("Humanoid")
    local root = char:FindFirstChild("HumanoidRootPart")
    if not humanoid or not root then return end
    BG = Instance.new("BodyGyro")
    BV = Instance.new("BodyVelocity")
    BG.MaxTorque = Vector3.new(9e9,9e9,9e9)
    BG.P = 9e4
    BG.CFrame = root.CFrame
    BG.Parent = root
    BV.MaxForce = Vector3.new(9e9,9e9,9e9)
    BV.Velocity = Vector3.zero
    BV.Parent = root
    humanoid.PlatformStand = true
    humanoid.AutoRotate = false
    flyConnection = RunService.RenderStepped:Connect(function()
        if not flyAtivo or not root.Parent then return end
        local cam = workspace.CurrentCamera
        local move = Vector3.zero
        if UserInputService:IsKeyDown(Enum.KeyCode.W) then move += cam.CFrame.LookVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.S) then move -= cam.CFrame.LookVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.A) then move -= cam.CFrame.RightVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.D) then move += cam.CFrame.RightVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.Space) then move += Vector3.new(0,1,0) end
        if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then move -= Vector3.new(0,1,0) end
        BV.Velocity = move.Magnitude>0 and move.Unit*flySpeed or Vector3.zero
        BG.CFrame = CFrame.new(root.Position, root.Position + cam.CFrame.LookVector)
    end)
end
local function setNoclip(state)
    noclipAtivo = state
    if noclipConnection then
        noclipConnection:Disconnect()
        noclipConnection = nil
    end
    if state then
        noclipConnection = RunService.Stepped:Connect(function()
            if LP.Character then
                for _, part in pairs(LP.Character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
        end)
    else
        if LP.Character then
            for _, part in pairs(LP.Character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = true
                end
            end
        end
    end
end
local function startAntiVoid(char)
    if antiVoidConnection then antiVoidConnection:Disconnect() end
    antiVoidConnection = RunService.Heartbeat:Connect(function()
        if not antiVoidAtivo or not char then return end
        local root = char:FindFirstChild("HumanoidRootPart")
        if root and root.Position.Y < 435 then
            root.CFrame = CFrame.new(root.Position.X, math.max(root.Position.Y, 435), root.Position.Z)
        end
    end)
end
local function stopAntiVoid()
    antiVoidAtivo = false
    if antiVoidConnection then
        antiVoidConnection:Disconnect()
        antiVoidConnection = nil
    end
end
local function onCharacter(char)
    char:WaitForChild("HumanoidRootPart")
    local humanoid = char:WaitForChild("Humanoid")
    humanoid.Died:Connect(function()
        if flyAtivo then
            flyWasActiveBeforeDeath = true
            flyAtivo = false
            safeStopFly()
            OrionLib:MakeNotification({Name="Fly",Content="âŒ Fly desativado! (vocÃª morreu!)",Image="rbxassetid://4483345998",Time=2})
        end
        if noclipAtivo then
            noclipWasActiveBeforeDeath = true
            setNoclip(false)
            OrionLib:MakeNotification({Name="Noclip",Content="âŒ Noclip desativado! (vocÃª morreu!)",Image="rbxassetid://4483345998",Time=2})
        end
        if antiVoidAtivo then
            antiVoidWasActiveBeforeDeath = true
            stopAntiVoid()
            OrionLib:MakeNotification({Name="Anti Void",Content="âŒ Anti Void desativado! (vocÃª morreu!)",Image="rbxassetid://4483345998",Time=2})
        end
    end)
    task.wait(0.1)
    if flyWasActiveBeforeDeath then
        flyAtivo = true
        startFly(char)
        OrionLib:MakeNotification({Name="Fly",Content="âœ… Fly reativado! (VocÃª Reespawnou!)",Image="rbxassetid://4483345998",Time=2})
        flyWasActiveBeforeDeath = false
    end
    if noclipWasActiveBeforeDeath then
        noclipAtivo = true
        setNoclip(true)
        OrionLib:MakeNotification({Name="Noclip",Content="âœ… Noclip reativado! (VocÃª Reespawnou!)",Image="rbxassetid://4483345998",Time=2})
        noclipWasActiveBeforeDeath = false
    end
    if antiVoidWasActiveBeforeDeath then
        antiVoidAtivo = true
        startAntiVoid(char)
        OrionLib:MakeNotification({Name="Anti Void",Content="âœ… Anti Void reativado! (VocÃª Reespawnou!)",Image="rbxassetid://4483345998",Time=2})
        antiVoidWasActiveBeforeDeath = false
    end
end
LP.CharacterAdded:Connect(onCharacter)
if LP.Character then onCharacter(LP.Character) end
FuncoesEspeciaisTab:AddToggle({
    Name = "Noclip",
    Default = false,
    Save = false,
    Flag = "noclip",
    Callback = function(state)
        setNoclip(state)
        if state then
            OrionLib:MakeNotification({Name="Noclip",Content="âœ… Noclip ativado!",Image="rbxassetid://4483345998",Time=2})
        else
            OrionLib:MakeNotification({Name="Noclip",Content="âŒ Noclip desativado!",Image="rbxassetid://4483345998",Time=2})
        end
    end
})
FuncoesEspeciaisTab:AddToggle({
    Name = "Fly",
    Default = false,
    Save = false,
    Flag = "fly",
    Callback = function(state)
        flyAtivo = state
        if state then
            if LP.Character then startFly(LP.Character) end
            OrionLib:MakeNotification({Name="Fly",Content="âœ… Fly ativado!",Image="rbxassetid://4483345998",Time=2})
        else
            safeStopFly()
            OrionLib:MakeNotification({Name="Fly",Content="âŒ Fly desativado!",Image="rbxassetid://4483345998",Time=2})
        end
    end
})
FuncoesEspeciaisTab:AddSlider({
    Name = "Fly Speed",
    Min = 0,
    Max = 2500,
    Default = 50,
    Color = Color3.fromRGB(255, 255, 255),
    Increment = 1,
    ValueName = "Fly Speed",
    Callback = function(Value)
        flySpeed = Value
    end
})
FuncoesEspeciaisTab:AddToggle({
    Name = "Anti Void",
    Default = false,
    Save = false,
    Flag = "anti_void",
    Callback = function(state)
        antiVoidAtivo = state
        if state then
            if LP.Character then startAntiVoid(LP.Character) end
            OrionLib:MakeNotification({Name="Anti Void",Content="âœ… Anti Void ativado! (Impede cair abaixo de Y=435)",Image="rbxassetid://4483345998",Time=2})
        else
            stopAntiVoid()
            OrionLib:MakeNotification({Name="Anti Void",Content="âŒ Anti Void desativado!",Image="rbxassetid://4483345998",Time=2})
        end
    end
})
