local noclipAtivo = false
local antiVoidAtivo = false
local flyWasActiveBeforeDeath = false
local noclipWasActiveBeforeDeath = false
local antiVoidWasActiveBeforeDeath = false
local flyConnection, antiVoidConnection, noclipConnection
local BG, BV
local function createTPButton(tab, name, pos)
    tab:AddButton({
        Name = name,
        Callback = function()
            if LP.Character and LP.Character:FindFirstChild("HumanoidRootPart") then
                local root = LP.Character:FindFirstChild("HumanoidRootPart")
                if noclipAtivo and root then
                    root.CFrame = CFrame.new(pos)
                else
                    LP.Character:MoveTo(pos)
                end
            else
                warn("Personagem nÃ£o encontrado ou carregando...")
            end
        end
    })
end
